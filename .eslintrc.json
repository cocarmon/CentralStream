{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
          "no-console": "warn",
          "no-unused-vars": "warn",
          "semi": "error",
          "indent": ["error", 2],
          "quotes": ["error", "single"],
          "no-undef": "error",
          "no-trailing-spaces": "error",
          "comma-dangle": ["error", "always-multiline"],
          "max-len": ["warn", { "code": 120 }],
          "no-multiple-empty-lines": ["error", { "max": 1 }]
      }
}
